{
  "name": "@openedx/frontend-app-base-test",
  "version": "0.1.0",
  "description": "A test application for frontend-base.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-app-base-test.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "dev:shell": "PORT=8082 openedx shell-dev-server",
    "build": "openedx webpack",
    "i18n_extract": "openedx formatjs extract",
    "lint": "openedx eslint .",
    "lint:fix": "openedx eslint --fix .",
    "snapshot": "openedx jest --updateSnapshot",
    "start": "openedx webpack-dev-server --progress",
    "start:with-theme": "paragon install-theme && npm start && npm install",
    "test": "openedx jest --coverage --passWithNoTests --no-cache",
    "serve": "openedx serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/openedx/frontend-app-base-test#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openedx/frontend-app-base-test/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@openedx/brand-openedx@^1.2.2",
    "@openedx/frontend-base": "file:../frontend-base/openedx-frontend-base-1.0.0.tgz",
    "@openedx/paragon": "^22.0.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^8.1.3",
    "react-router": "6.23.1",
    "react-router-dom": "^6.25.1",
    "redux": "4.2.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.2.0",
    "@edx/reactifex": "^2.1.1",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.7.0",
    "react-test-renderer": "^17.0.2"
  }
}
